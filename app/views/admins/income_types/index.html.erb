  <h1>収入カテゴリ一覧</h1>
  <div class="form-group">
    <%= form_with model:[:admins, @income_type], local: true do |f| %>
      <tr>
        <th>収入カテゴリ名</th>
        <th><%= f.text_field :type_name %></th>
        <th><%= f.submit '新規登録', class: 'btn btn-primary' %></th>
      </tr>
    <% end %>
  </div>
  <table class="table table-bordered mt-5">
    <% @income_types.each do |type| %>
      <tr>
        <td><%= type.type_name %></td>
        <td><%= link_to "編集する", edit_admins_income_type_path(type), class:'btn btn-success' %></td>
        <td><%= link_to "削除する", admins_income_type_path(type), method: :delete, class:'btn btn-danger' %></td>
      </tr>
    <% end %>
  </table>

